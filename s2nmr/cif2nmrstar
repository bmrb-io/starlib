   	   C
		"_citation_editor.name"  L
		stop_
            "_citation.id"			  	# _input_key_tag
            *          				# _input_sf_name
            *				        # _input_sf_key_tag
            *		    			# _input_sf_key_value

            citation                # _output_sf_category
            "save_citation"           # _output_sf_base_name
            "_citation.id"  		    # _output_sf_name_tag
            C                               # _copy_or_move_flag

                # List of _input_tag values:
		"_citation.title"   F
		"_citation.journal_abbrev"   F
